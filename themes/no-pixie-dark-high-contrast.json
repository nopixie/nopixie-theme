  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A0A0A0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF00FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#FF80FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#80C8FF"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#FFFF00",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#80C8FF"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF4444",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#FFFF00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#FFFF00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#FF80FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#80C8FF",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#A0A0A0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "scope": [
        "entity.name.type.alias",
        "entity.name.type.interface",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": ["entity.name.type.parameter"],
      "settings": {
        "foreground": "#FFFF00",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.type.annotation", "meta.return.type"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": ["support.type.builtin"],
      "settings": {
        "foreground": "#00FFFF",
        "fontStyle": "italic"
      }
    }
  ]
}
