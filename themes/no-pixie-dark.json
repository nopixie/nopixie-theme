  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8A8F98",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#A0D080"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#FFD84D"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#7BD1CF"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#A06BA0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#C086BD",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#D8DDE5"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#A0B8D8"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#FFE050"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#F0F3F8"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#FFD84D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#7BD1CF"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#A06BA0"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#A0B8D8"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF7B7B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#FFE050",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#FFD84D",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#C086BD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#A0B8D8",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#8A8F98",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#A0D080"
      }
    },
    {
      "scope": [
        "entity.name.type.alias",
        "entity.name.type.interface",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#7BD1CF"
      }
    },
    {
      "scope": ["entity.name.type.parameter"],
      "settings": {
        "foreground": "#FFD84D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.type.annotation", "meta.return.type"],
      "settings": {
        "foreground": "#7BD1CF"
      }
    },
    {
      "scope": ["support.type.builtin"],
      "settings": {
        "foreground": "#7BD1CF",
        "fontStyle": "italic"
      }
    }
  ]
}
