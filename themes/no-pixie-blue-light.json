  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5A6570",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#1A7A5A"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#1A5F8F"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#1A7F7D"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#2A5078",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#4A7FB8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#4A5260"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#2A5078",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#1A5F8F"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#2A3340"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#1A5F8F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#1A7F7D"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#2A5078"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#2A5078"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#C42A2A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#2A5078",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#1A5F8F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#4A7FB8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#2A5078",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#6A7580",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#1A7A5A"
      }
    },
    {
      "scope": [
        "entity.name.type.alias",
        "entity.name.type.interface",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#1A7F7D"
      }
    },
    {
      "scope": ["entity.name.type.parameter"],
      "settings": {
        "foreground": "#1A5F8F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.type.annotation", "meta.return.type"],
      "settings": {
        "foreground": "#1A7F7D"
      }
    },
    {
      "scope": ["support.type.builtin"],
      "settings": {
        "foreground": "#1A7F7D",
        "fontStyle": "italic"
      }
    }
  ]
}
