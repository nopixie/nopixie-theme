{
  "name": "nopixie-theme",
  "displayName": "No Pixie theme",
  "description": "A minimal theme for VS Code",
  "version": "0.5.0",
  "publisher": "no-pixie",
  "icon": "images/icon.jpg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nopixie/nopixie-theme"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "No Pixie Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/no-pixie-dark.json"
      },
      {
        "label": "No Pixie Dark High Contrast",
        "uiTheme": "vs-dark",
        "path": "./themes/no-pixie-dark-high-contrast.json"
      },
      {
        "label": "No Pixie Light",
        "uiTheme": "vs",
        "path": "./themes/no-pixie-light.json"
      },
      {
        "label": "No Pixie Light High Contrast",
        "uiTheme": "vs",
        "path": "./themes/no-pixie-light-high-contrast.json"
      }
    ]
  },
  "keywords": [
    "theme",
    "color-theme",
    "dark",
    "light",
    "cozy"
  ],
  "scripts": {
    "prepare": "husky",
    "package": "vsce package",
    "publish:vsce": "vsce publish",
    "publish:ovsx": "ovsx publish",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@vscode/vsce": "^2.22.0",
    "conventional-changelog-cli": "5.0.0",
    "husky": "9.1.7",
    "ovsx": "^0.10.6"
  }
}
